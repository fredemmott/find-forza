{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/nologo",
                "/std:c++20",
                "/Fe${fileDirname}\\${fileBasenameNoExtension}.exe",
                "ole32.lib",
                "oleaut32.lib",
                "Kernel32.lib",
                "${file}",
                "/link",
                "/MANIFEST:EMBED",
                "/MANIFESTINPUT:manifest.xml"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}